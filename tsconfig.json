{
	"compilerOptions": {
		"lib": ["dom", "dom.Iterable", "esnext"],
		"jsx": "react-jsx",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"noEmit": true,
		"allowJs": true,
		"isolatedModules": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	},
	"include": ["src"]
}
